{
	"version": 2,
	"settings": {
		"border": {
			"color": true,
			"radius": true,
			"style": true,
			"width": true
		},
		"color": {
			"background": true,
			"text": true,
			"link": true,
			"custom": true,
			"customDuotone": true,
			"customGradient": true,
			"defaultGradients": true,
			"defaultPalette": true,
			"palette": [
				{
					"slug": "primary",
					"color": "#033E3E",
					"name": "Primary"
				},
				{
					"slug": "secondary",
					"color": "darkslategray",
					"name": "Secondary"
				},
				{
					"slug": "general-text",
					"color": "black",
					"name": "General Text"
				},
				{
					"slug": "general-background",
					"color": "black",
					"name": "General Background"
				},
				{
					"slug": "secondary-background",
					"color": "gainsboro",
					"name": "Secondary Background"
				},
				{
					"slug": "block-background",
					"color": "mintcream",
					"name": "Block Background"
				}
			]
		},
		"custom": {
			"spacing": {
				"default-gap": "0.5rem",
				"elements": "0.5rem"
			},
			"typography": {
				"line-height": {
					"headings": 1.25,
					"text": 1.45
				},
				"font-size": {
					"body": "17px",
					"text": "clamp(16px, calc(1rem + 0.16vw), 18px)",
					"button": "clamp(14px, calc(0.875rem + 0.16vw), 16px)"
				}
			}
		},
		"layout": {
			"contentSize": "calc(1360px * 0.7 - 1rem - 2.5vw)"
		},
		"typography": {
			"dropCap": true,
			"customFontSize": true,
			"letterSpacing": true,
			"lineHeight": true,
			"fontStyle": true,
			"fontWeight": true,
			"textTransform": true,
			"fontFamilies": [
				{
					"fontFamily": "Franklin Gothic Medium, Arial Narrow, Arial, sans-serif",
					"name": "DerekFont",
					"slug": "derekfont"
				}
			],
			"fontSizes": [
				{
					"size": "13px",
					"slug": "tiny",
					"name": "Tiny"
				},
				{
					"size": "15px",
					"slug": "x-small",
					"name": "Extra Small"
				},
				{
					"size": "16px",
					"slug": "small",
					"name": "Small"
				},
				{
					"size": "18px",
					"slug": "regular",
					"name": "Regular"
				},
				{
					"size": "clamp(20px, calc(1rem + 0.83vw), 22px)",
					"slug": "medium",
					"name": "Medium"
				},
				{
					"size": "clamp(24px, calc(1rem + 1.25vw), 26px)",
					"slug": "large",
					"name": "Large"
				},
				{
					"size": "clamp(28px, calc(1rem + 1.67vw), 30px)",
					"slug": "x-large",
					"name": "Extra Large"
				},
				{
					"size": "clamp(32px, calc(1rem + 2.5vw), 34px)",
					"slug": "xx-large",
					"name": "Extra Extra Large"
				},
				{
					"size": "clamp(36px, calc(1rem + 2.75vw), 42px)",
					"slug": "xxx-large",
					"name": "Extra Extra Extra Large"
				}
			]
		},
		"spacing": {
			"blockGap": "var(--wp--custom--spacing--default-gap)",
			"padding": true,
			"margin": true,
			"units": [ "px", "em", "rem", "vh", "vw", "%" ]
		}
	},
	"styles": {
		"blocks": {
			"core/button": {
				"border": {
					"radius": "5px"
				},
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--block-background)"
				},
				"typography": {
					"fontSize": "var(--wp--custom--typography--font-size--button)"
				}
			},
			"core/columns": {
				"spacing": {
					"blockGap": "calc(0.5rem + 1.25vw)"
				}
			},
			"core/site-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--derekfont)",
					"fontSize": "var(--wp--preset--font-size--xxx-large)",
					"fontWeight": "700",
					"lineHeight": "var(--wp--custom--typography--line-height--headings)"
				}
			},
			"core/post-title": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			},
			"core/post-content": {
				"typography": {
					"fontSize": "var(--wp--custom--typography--font-size--text)"
				}
			},
			"core/post-excerpt": {
				"typography": {
					"fontSize": "var(--wp--custom--typography--font-size--text)"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--block-background)",
			"text": "var(--wp--preset--color--general-text)"
		},
		"elements": {
			"h1": {
				"spacing": {
					"margin": {
						"top": "1.5rem",
						"bottom": "1rem"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--derekfont)",
					"fontSize": "var(--wp--preset--font-size--xx-large)",
					"lineHeight": "var(--wp--custom--typography--line-height--headings)"
				}
			},
			"h2": {
				"spacing": {
					"margin": {
						"top": "1.5rem",
						"bottom": "1rem"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--derekfont)",
					"fontSize": "var(--wp--preset--font-size--x-large)",
					"lineHeight": "var(--wp--custom--typography--line-height--headings)"
				}
			},
			"h3": {
				"spacing": {
					"margin": {
						"top": "1.5rem",
						"bottom": "1rem"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--derekfont)",
					"fontSize": "var(--wp--preset--font-size--large)",
					"lineHeight": "var(--wp--custom--typography--line-height--headings)"
				}
			},
			"h4": {
				"spacing": {
					"margin": {
						"top": "1.5rem",
						"bottom": "1rem"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--derekfont)",
					"fontSize": "var(--wp--preset--font-size--medium)",
					"lineHeight": "var(--wp--custom--typography--line-height--headings)"
				}
			},
			"h5": {
				"spacing": {
					"margin": {
						"top": "1.5rem",
						"bottom": "1rem"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--derekfont)",
					"fontSize": "22px",
					"lineHeight": "var(--wp--custom--typography--line-height--headings)"
				}
			},
			"h6": {
				"spacing": {
					"margin": {
						"top": "1.5rem",
						"bottom": "1rem"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--derekfont)",
					"fontSize": "20px",
					"lineHeight": "var(--wp--custom--typography--line-height--headings)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			}
		},
		"typography": {
			"fontSize": "var(--wp--custom--typography--font-size--body)",
			"fontFamily": "var(--wp--preset--font-family--derekfont)",
			"lineHeight": "var(--wp--custom--typography--line-height--text)"
		},
		"spacing": {
			"blockGap": "var(--wp--custom--spacing--default-gap)"
		}
	},
	"templateParts": [
		{
			"name": "header",
			"title": "Header",
			"area": "header"
		},
		{
			"name": "footer",
			"title": "Footer",
			"area": "footer"
		},
		{
			"name": "sidebar",
			"title": "Sidebar",
			"area": "uncategorized"
		}
	]
}
